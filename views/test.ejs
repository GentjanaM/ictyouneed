<% include partials/header%>


    <div class="well">

        <table>

            <tr>
                <h4>
                <% users.forEach(function(user) { %>
                <th><%= user.user_id %></th>
                <% }) %>
                </h4>
            </tr>
            <% users.forEach(function(user) { %>
            <tr>
                <td>
                    <%= user.firstname %>
                </td>
                <td>
                    <%= user.lastname %>
                </td>
                <td>
                    <%= user.email %>
                </td>
                <td>
                    <%= user.role %>
                </td>
                <td>
                    <%= user.location %>
                </td>
            </tr>
            <% }) %>
        </table>

    </div>


<% include partials/footer %>